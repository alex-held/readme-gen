{
  "name": "readme-gen",
  "version": "1.0.0",
  "description": "CLI that generates beautiful README.md files.",
  "main": "src/index.js",
  "bin": {
    "readme-gen": "src/index.js"
  },
  "dependencies": {
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "boxen": "^4.0.0",
    "date-fns": "^2.0.1",
    "ejs": "^3.0.1",
    "git-repo-name": "^1.0.1",
    "inquirer": "~7.0.0",
    "load-json-file": "^6.0.0",
    "lodash": "^4.17.11",
    "markdown-escape": "^1.0.2",
    "node-fetch": "^2.6.0",
    "ora": "4.0.3",
    "readme-md-generator": "^1.0.0",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.18",
    "tslib": "^1.10.0",
    "yargs": "^15.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.21",
    "codecov": "^3.5.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.17.3",
    "jest": "^24.8.0",
    "nodejs": "^0.0.0",
    "npm": "^7.6.3",
    "prettier": "^1.17.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier \"**/*.{js,md,json}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage && codecov",
    "tsc:w": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-held/readme-gen.git"
  },
  "keywords": [
    "readme",
    "md",
    "cli",
    "generator",
    "template"
  ],
  "author": "alex-held",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kefranabg/readme-md-generator/issues"
  },
  "homepage": "https://github.com/kefranabg/readme-md-generator#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@alex-held"
  },
  "engines": {
    "npm": ">=5.5.0",
    "node": ">=9.3.0"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)",
      "**/__tests__/**/*.js?(x)"
    ]
  }
}
